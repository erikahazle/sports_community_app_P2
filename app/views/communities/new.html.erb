<%= form_for [@sport, @community] do |f| %>

  <%= f.label :name  %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :post_code %>
  <%= f.text_field :post_code %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.submit 'Submit' %>

<% end %>